{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Settings for the virtual network building block",
    "id": "https://raw.githubusercontent.com/mspnp/template-building-blocks/master/schemas/extensions/types.json#",
    "definitions": {
        "protectedSettingsStorageAccount": {
            "properties": {
                "storageAccountName": {
                    "type": "string"
                },
                "storageAccountKey": {
                    "type": "string"
                }
            }
        },
        "settingsFileUris": {
            "properties": {
                "fileUris": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}